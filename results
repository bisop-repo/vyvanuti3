#!/bin/bash

rm v3/report/results/*.*
./copy df_fin txt T 
./copy est_table txt E 
./copy heatmap pdf H 
./copy heatmapnk pdf N 
./copy heatmapshort pdf HS
./copy heatmapshortnk pdf NS
./copy heatmaplong pdf HL
./copy heatmaplongnk pdf NL
./copy r_fin_mat txt M
./copy z_score_fin_mat txt Z
./copy short_fin_mat txt MS
./copy short_z_score_fin_mat txt ZS
./copy long_fin_mat txt ML
./copy long_z_score_fin_mat txt ZL
cp v3/ProxyBA12/cox_model_summary.txt v3/report/results/ProxyBA12C.TXT
cp v3/ProxyNew/cox_model_summary.txt v3/report/results/ProxyNewC.TXT
# cp v3/LCallO/cox_model_summary.txt v3/report/results/LCAllC.TXT
cp v3/LCinfO/logreg_model_summary.txt v3/report/results/LCInfL.TXT
cd v3
cd report
pdflatex report.tex



